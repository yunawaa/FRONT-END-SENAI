algoritmo "UNTIL - valores negativos"
var
           contador, numero, negativo : inteiro
inicio
           escreval ("Digite 10 números:")
           repita
           leia (numero)
           
           se (numero < 0) entao
           negativo <- negativo + 1
           fimSe
           
           contador <- contador - 1



           ate ( contador >= 10)
fimalgoritmo