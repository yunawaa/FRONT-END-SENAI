algoritmo "soma 10 números"
var
          numero, contador, somaTotal : inteiro
inicio
          escreval ("Digite 10 números.")
          enquanto ( contador < 10 ) faca
          leia (numero)
          somaTotal <- somaTotal + numero
          contador <- contador + 1
          fimEnquanto
          escreval ("A soma total dos valores é de ", somaTotal)
fimalgoritmo