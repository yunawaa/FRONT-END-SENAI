algoritmo "múltiplo de 10"
var
          contador : inteiro
inicio
          contador <- 1
          enquanto (contador < 100) faca
          se (contador % 10 = 0) entao
          escreval (contador, " É múltiplo de 10!")
          senao
          escreval (contador)
          fimSe
          contador <- contador + 1
          fimEnquanto
          
          //
fimalgoritmo