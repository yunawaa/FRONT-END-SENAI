algoritmo "ATVD 2 FLUXOGRAMA, verifica positivo ou negativo"
var
       numero : real
       verifica : logico
inicio
       escreval ("Digite um número para verificar se é positivo.")
       leia (numero)
       se (numero >= 0) entao
       escreval ("Número positivo")
       senao
       escreval ("Número negativo")
       fimSe
fimalgoritmo